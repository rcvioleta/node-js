</head>

<body>
    <%- include('../includes/navigation.ejs') %>
    <br><br><br>
    <main>
         <% if (products.length > 0) { %>
            <ul>
                <% products.forEach(p => { %> 
                    <li><strong><%= p.productData.title %></strong> - (<%= p.qty %>)</li>
                <% }) %>
            </ul>
         <% } else { %>
            <h1>No Items added to your Cart!</h1>
         <% } %>
    </main>
<%- include('../includes/end.ejs') %>